CFLAGS=-W -Wall -std=c11 -ggdb -pthread -m32
LIBS=
SRCS= #<<< TODO: PUT YOUR C SOURCE CODE FILE(S) HERE - BEFORE THE COMMENT - DELETE THIS COMMENT AFTER YOU'VE DONE THIS!
OBJS=$(SRCS:.c=.o)
TARGET= #<<< TODO: PUT THE NAME OF YOUR TARGET EXECUTABLE HERE - BEFORE THE COMMENT - DELETE THIS COMMENT AFTER YOU'VE DONE THIS!

$(TARGET): $(OBJS)
	$(CC) $(CFLAGS) -o $(TARGET) $(OBJS) $(LIBS)

.PHONY: clean
